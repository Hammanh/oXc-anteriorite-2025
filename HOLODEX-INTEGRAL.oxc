  bindings_rust: |
    /// Structure principale de l'application YATASANA
    pub struct Yatasana {
        pub oracle: OracleModule,
        pub bibliholo: BibliHoloModule,
        pub sante: SanteModule,
        pub projets: ProjetsModule,
        pub user_profile: UserProfile,
        pub prolog_engine: ScryerEngine,
    }

    impl Yatasana {
        pub fn new(user_key: &HolochainKey) -> Self {
            // Charger le profil utilisateur
            let profile = UserProfile::load(user_key);

            // Initialiser le moteur Prolog
            let prolog = ScryerEngine::new();
            prolog.load_holodex("HOLODEX-INTEGRAL.oxc");

            Self {
                oracle: OracleModule::new(&prolog),
                bibliholo: BibliHoloModule::new(&prolog),
                sante: SanteModule::new(&prolog),
                projets: ProjetsModule::new(&prolog),
                user_profile: profile,
                prolog_engine: prolog,
            }
        }

        pub fn etablir_resonance(&mut self, freq: u32) {
            self.user_profile.set_frequency(freq);
        }
    }

