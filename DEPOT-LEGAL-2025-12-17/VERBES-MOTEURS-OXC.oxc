        ; F1 mod F2 =:= 0 -> Score = 0.9
        ; F2 mod F1 =:= 0 -> Score = 0.9
        ; Score is max(0, 1.0 - (Diff / 1000))
        ).

    % ACTIVATION DU FLUX
    activer_flux(Holon, Verbe) :-
        verbe_moteur(Verbe, _, _),
        monnaies_associees(Verbe, M1, M2),
        ouvrir_wallet(Holon, M1),
        ouvrir_wallet(Holon, M2).

