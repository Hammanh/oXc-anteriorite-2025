    .freq-badge {
      display: inline-block;
      padding: 4px 12px;
      border-radius: 20px;
      font-weight: bold;
      font-size: 14px;
    }

    .freq-528 { background: var(--freq-528); color: black; }
    .freq-639 { background: var(--freq-639); color: white; }
    .freq-741 { background: var(--freq-741); color: white; }
    .freq-852 { background: var(--freq-852); color: white; }

