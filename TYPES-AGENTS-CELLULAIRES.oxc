      transduction_frequence(528, guerisseur).  % MÃªme frÃ©quence, contexte diffÃ©rent
      transduction_frequence(639, tisserand).
      transduction_frequence(852, oracle).
      transduction_frequence(417, alchimiste).
      transduction_frequence(432, nourricier).
      transduction_frequence(432, veilleur).    % 432 Hz par dÃ©faut = VEILLEUR

      % DÃ©sambiguÃ¯sation 528 Hz (BÃ¢tisseur vs GuÃ©risseur)
      differencier_528(Agent, batisseur) :-
          frequence_ambiante(528),
          contexte_actuel(creation).

