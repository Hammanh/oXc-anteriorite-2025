      % --- ACTIVATION DU TYPE ---

      activer_type(Agent, Type) :-
          retract(type_cellulaire(Agent, _)),
          assert(type_cellulaire(Agent, Type)),
          charger_capacites(Agent, Type),
          inhiber_capacites_autres(Agent, Type).

      charger_capacites(Agent, Type) :-
          retractall(capacite_active(Agent, _)),
          findall(C, capacite(Type, C), Capacites),
          forall(member(Cap, Capacites), assert(capacite_active(Agent, Cap))).

      inhiber_capacites_autres(Agent, TypeActif) :-
          findall(T, (type_agent(T), T \= TypeActif, T \= indifferencie, T \= veilleur), AutresTypes),
          forall(member(AT, AutresTypes), inhiber_capacites_type(Agent, AT)).

