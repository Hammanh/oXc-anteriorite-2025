      /* Couches */
      --couche-1-bg: linear-gradient(180deg, #F5F5F5 0%, #D0D0D0 100%);
      --couche-2-bg: linear-gradient(180deg, #90EE90 0%, #32CD32 100%);
      --couche-3-bg: linear-gradient(180deg, #EE82EE 0%, #9932CC 100%);
    }

    /* Agent Cards */
    .agent-card {
      border-radius: 12px;
      padding: 16px;
      margin: 8px;
      transition: all 0.3s ease;
    }

    .agent-card:hover {
      transform: translateY(-4px);
    }

