      background: linear-gradient(135deg, #8B00FF 0%, #4B0082 100%);
      box-shadow: 0 0 20px rgba(139, 0, 255, 0.5);
      color: white;
    }

    .agent-batisseur {
      background: linear-gradient(135deg, #00FF00 0%, #228B22 100%);
      box-shadow: 0 0 20px rgba(0, 255, 0, 0.5);
    }

    .agent-tisserand {
      background: linear-gradient(135deg, #0099FF 0%, #0066CC 100%);
      box-shadow: 0 0 20px rgba(0, 153, 255, 0.5);
      color: white;
    }

    .agent-oracle {
      background: linear-gradient(135deg, #4B0082 0%, #2E0854 100%);
      box-shadow: 0 0 25px rgba(75, 0, 130, 0.6);
