    %% Actions du cycle
    recoit(Etre, Savoir) :-
        peut_recevoir(Etre),
        format("~w REÃ‡OIT: ~w~n", [Etre, Savoir]).
    
    integre(Etre, Savoir, SavoirTransforme) :-
        peut_integrer(Etre),
