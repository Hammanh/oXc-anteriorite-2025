    %% L'Holocyclie est un cycle de 3 phases
    holocyclie(Cycle) :-
        Cycle = cycle(recevoir, integrer, offrir),
        phase(recevoir, 1),
        phase(integrer, 2),
        phase(offrir, 3).
    
    %% â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€
    %% RÃˆGLES DU CYCLE
